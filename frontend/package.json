{
  "name": "rustwebui-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run rsbuild dev --config rsbuild.config.dev.ts",
    "build": "bun run ../build-frontend.js",
    "build:incremental": "bun run rsbuild build --config rsbuild.config.ts",
    "preview": "bun run rsbuild preview --config rsbuild.config.dev.ts",
    "clean": "rm -rf dist && rm -rf node_modules/.cache",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:ui": "bun test --watch --coverage",
    "lint": "bun run biome lint",
    "lint:fix": "bun run biome lint --write",
    "lint:ci": "bun run biome lint --max-diagnostics=100",
    "format": "bun run biome format",
    "format:fix": "bun run biome format --write",
    "format:check": "bun run biome format --check",
    "check": "bun run biome check",
    "check:fix": "bun run biome check --write",
    "check:ci": "bun run biome ci",
    "validate": "bun run biome check && bun run tsc --noEmit",
    "validate:fix": "bun run biome check --write && bun run tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "winbox": "^0.2.82"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@rsbuild/core": "^1.4.0",
    "@rsbuild/plugin-react": "^1.4.5",
    "@types/bun": "^1.2.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.0.0"
  }
}